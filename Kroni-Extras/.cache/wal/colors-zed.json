{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#11101890",
                "editor.background": "#11101890",
                "editor.foreground": "#c3c3c5",
                "text": "#c3c3c5",
                "text.muted": "#c3c3c570",
                "text.ignored": "#c3c3c540",
                "text.placeholder": "#c3c3c550",
                "ignored": "#c3c3c530",
                "element.hover": "#c3c3c530",
                "ghost_element.hover": "#445A9030",
                "ghost_element.selected": "#445A9030",
                "ghost_element.active": "#445A9060",
                "border": "#111018",
                "editor.highlighted_line.background": "#445A9010",
                "editor.active_line.background": "#445A9010",
                "panel.background": "#11101890",
                "title_bar.background": "#11101890",
                "title_bar.inactive_background": "#11101890",
                "status_bar.background": "#11101890",
                "drop_target.background": "#11101890",
                "elevated_surface.background": "#111018",
                "toolbar.background": "#11101890",
                "tab_bar.background": "#11101890",
                "tab.inactive_background": "#11101890",
                "tab.active_background": "#445A9030",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#111018",
                "scrollbar.thumb.background": "#445A90",
                "editor.gutter.background": "#11101890",
                "terminal.background": "#11101810",
                "terminal.foreground": "#c3c3c5",
                "terminal.dim_foreground": "#c3c3c5",
                "terminal.bright_foreground": "#c3c3c5",
                "terminal.ansi.black": "#111018",
                "terminal.ansi.red": "#445A90",
                "terminal.ansi.green": "#5D6984",
                "terminal.ansi.yellow": "#6E748E",
                "terminal.ansi.blue": "#897F98",
                "terminal.ansi.magenta": "#9C93AB",
                "terminal.ansi.cyan": "#CBB5BB",
                "terminal.ansi.white": "#c3c3c5",
                "terminal.ansi.bright_black": "#5f5b6f",
                "terminal.ansi.bright_red": "#445A90",
                "terminal.ansi.bright_green": "#5D6984",
                "terminal.ansi.bright_yellow": "#6E748E",
                "terminal.ansi.bright_blue": "#897F98",
                "terminal.ansi.bright_magenta": "#9C93AB",
                "terminal.ansi.bright_cyan": "#CBB5BB",
                "terminal.ansi.bright_white": "#c3c3c5",
                "modified": "#445A90",
                "syntax": {
                    "attribute": {
                        "color": "#445A90"
                    },
                    "boolean": {
                      "color": "#5D6984"
                    },
                    "comment": {
                      "color": "#5f5b6f"
                    },
                    "comment.doc": {
                      "color": "#5f5b6f"
                    },
                    "constant": {
                      "color": "#5D6984"
                    },
                    "function": {
                      "color": "#CBB5BB"
                    },
                    "keyword": {
                      "color": "#897F98"
                    },
                    "number": {
                      "color": "#9C93AB"
                    },
                    "operator": {
                      "color": "#897F98"
                    },
                    "string": {
                      "color": "#445A90"
                    },
                    "variable": {
                      "color": "#5D6984"
                    }
                },
                "players": [
                  {
                    "cursor": "#445A90",
                    "background": "#111018",
                    "selection": "#445A9030"
                  }
                ]
            }
        }
    ]
}
